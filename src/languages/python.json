{
    "number": {
        "regexes": [
            "[0-9]+"
        ]
    },
    "variable": {
        "regexes": [
            {
                "regexString": "\\.(\\w+)\\b",
                "captureGroup": 1
            }
        ]
    },
    "function": {
        "regexes": [
            "\\b[a-zA-Z_][a-zA-Z0-9_-]+?(?=\\()"
        ]
    },
    "keyword": {
        "regexes": [
            "\\band\\b",
            "\\bas\\b",
            "\\bassert\\b",
            "\\bbreak\\b",
            "\\bclass\\b",
            "\\bcontinue\\b",
            "\\bdef\\b",
            "\\belif\\b",
            "\\belse\\b",
            "\\bexcept\\b",
            "\\bexec\\b",
            "\\bfinally\\b",
            "\\bfor\\b",
            "\\bfrom\\b",
            "\\bglobal\\b",
            "\\bif\\b",
            "\\bimport\\b",
            "\\bin\\b",
            "\\bis\\b",
            "\\blambda\\b",
            "\\bnot\\b",
            "\\bor\\b",
            "\\bpass\\b",
            "\\bprint\\b",
            "\\braise\\b",
            "\\breturn\\b",
            "\\btry\\b",
            "\\bwhile\\b",
            "\\bwith\\b",
            "\\byield\\b"
        ]
    },
    "literal-values": {
        "regexes": [
            "\\bTrue\\b",
            "\\bFalse\\b",
            "\\bNone\\b"
        ]
    },
    "comment": {
        "regexes": [
            "#.+",
            "\\/\\*[^\\*\\/]+\\*\\/"
        ]
    },
    "wrapping": {
        "regexes": [
            "\"(\\\\.|[^\"])*\"",
            "'.?'"
        ],
        "precedence": "comment"
    }
}
